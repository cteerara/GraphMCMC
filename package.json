{
  "name": "mcmc",
  "version": "0.0.0",
  "description": "Markov Chain Monte Carlo program to estimate distribution network",
  "homepage": "",
  "author": {
    "name": "Chayut Teeraratkul",
    "email": "cteerara@u.rochester.edu",
    "url": ""
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "keywords": [
    "Markov Chain",
    "Monte Carlo"
  ],
  "devDependencies": {
    "coveralls": "^3.0.0",
    "eslint": "^5.4.0",
    "prettier": "^1.11.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-config-xo": "^0.24.2",
    "jest": "^23.5.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "git add",
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "env": {
      "jest": true,
      "node": true
    },
    "extends": [
      "prettier",
      "xo"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        {
          "singleQuote": true,
          "printWidth": 90
        },
        "error"
      ]
    }
  },
  "scripts": {
    "precommit": "lint-staged",
    "test": "jest"
  },
  "repository": "git@github.com:cteerara/cteerara.git",
  "jest": {
    "testEnvironment": "node"
  },
  "license": "MIT"
}
